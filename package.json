{
  "name": "skyflux-api",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "AGPL-3.0-only",
  "scripts": {
    "start": "ts-node-dev -r dotenv/config src/index.ts",
    "pretty-quick": "pretty-quick --pattern \"**/*.*(js|ts)",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@types/node": "^14.6.4",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.8.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "module-alias": "^2.2.2",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.1",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pretty-quick && yarn lint --max-warnings=0"
    }
  },
  "_moduleAliases": {
    "@models": "src/models",
    "@utils": "src/utils"
  },
  "dependencies": {
    "@types/mongoose": "^5.7.36",
    "@types/uuid": "^8.3.0",
    "apollo-server": "^2.17.0",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.2.1",
    "mongoose": "^5.10.4",
    "uuid": "^8.3.0"
  }
}
